[Desktop Entry]
Type=Application
Name={{ APP_DISPLAY_NAME }}
GenericName={{ APP_GENERIC_NAME }}
Comment={{ APP_COMMENT }}
Icon={{ APP_ICON }}
Exec=env LD_LIBRARY_PATH={{ APP_PREFIX }}/lib:{{ APP_PREFIX }}/lib64 {{ APP_EXEC }}
Terminal=false
Categories={{ APP_CATEGORIES }}
Keywords={{ APP_KEYWORDS }}

